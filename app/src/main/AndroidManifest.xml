<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.goodweather">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:name="com.example.goodweather.base.UniteApp"
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.goodweather.ContactActivity"></activity>
        <activity android:name="com.example.goodweather.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.goodweather.city_manager.CityManagerActivity" />
        <activity android:name="com.example.goodweather.city_manager.SearchCityActivity" />
        <activity android:name="com.example.goodweather.city_manager.DeleteCityActivity" />
        <activity android:name="com.example.goodweather.MoreActivity" />
    </application>

</manifest>